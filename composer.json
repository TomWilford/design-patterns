{
  "name": "tomwilford/design-patterns",
  "autoload": {
    "psr-4": {
      "Creational\\Factory\\Conceptual\\": "creational/Factory/Conceptual",
      "Creational\\Factory\\Practical\\": "creational/Factory/Practical",
      "Creational\\AbstractFactory\\Conceptual\\": "creational/AbstractFactory/Conceptual",
      "Creational\\AbstractFactory\\Practical\\": "creational/AbstractFactory/Practical",
      "Creational\\Singleton\\Conceptual\\": "creational/Singleton/Conceptual",
      "Creational\\Singleton\\Practical\\": "creational/Singleton/Practical",
      "Creational\\Builder\\Conceptual\\": "creational/Builder/Conceptual",
      "Creational\\Builder\\Practical\\": "creational/Builder/Practical",
      "Creational\\Prototype\\Conceptual\\": "creational/Prototype/Conceptual",
      "Creational\\Prototype\\Practical\\": "creational/Prototype/Practical",
      "Structural\\Adapter\\Conceptual\\": "structural/Adapter/Conceptual",
      "Structural\\Adapter\\Practical\\": "structural/Adapter/Practical"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fixtures\\": "tests/Fixtures",
      "TestCase\\Creational\\Factory\\Conceptual\\": "tests/TestCase/Creational/Factory/Conceptual",
      "TestCase\\Creational\\Factory\\Practical\\": "tests/TestCase/Creational/Factory/Practical",
      "TestCase\\Creational\\AbstractFactory\\Conceptual\\": "tests/TestCase/Creational/AbstractFactory/Conceptual",
      "TestCase\\Creational\\AbstractFactory\\Practical\\": "tests/TestCase/Creational/AbstractFactory/Practical",
      "TestCase\\Creational\\Singleton\\Practical\\": "tests/TestCase/Creational/Singleton/Practical",
      "TestCase\\Creational\\Builder\\Conceptual\\": "tests/TestCase/Creational/Builder/Conceptual",
      "TestCase\\Creational\\Builder\\Practical\\": "tests/TestCase/Creational/Builder/Practical",
      "TestCase\\Creational\\Prototype\\Conceptual\\": "tests/TestCase/Creational/Prototype/Conceptual",
      "TestCase\\Practical\\Prototype\\Conceptual\\": "tests/TestCase/Creational/Prototype/Practical",
      "TestCase\\Structural\\Adapter\\Conceptual\\": "tests/TestCase/Structural/Adapter/Conceptual",
      "TestCase\\Structural\\Adapter\\Practical\\": "tests/TestCase/Structural/Adapter/Practical"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "require": {
    "symfony/emoji": "^8",
    "psr/http-message": "^2.0",
    "nyholm/psr7": "^1.8"
  },
  "scripts": {
    "unit": "./vendor/bin/phpunit",
    "stan": "./vendor/bin/phpstan",
    "cs-scan": "./vendor/bin/phpcs",
    "cs-fix": "./vendor/bin/phpcbf",
    "all-tests": [
      "@unit",
      "@stan",
      "@cs-scan"
    ]
  }
}
